FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      zoneID;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 0;
boundaryField
{
    sides
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    domain
    {
        type            overset;
        value           uniform 1;
    }
    propellerCW
    {
        type            zeroGradient;
    }
    domain_PropellerMesh_CCW
    {
        type            overset;
        value           uniform 2;
    }
    propellerCCW
    {
        type            zeroGradient;
    }
}


